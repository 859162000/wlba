doctype html
- with '基金产品' as title
include include/head
- endwith
body
  include include/header
  - with '基金产品' as activeItem
  include include/navigation
  - endwith
  .sub-navbar
    .sub-navbar--list
      ul
        li.sub-navbar--list-item
          a.sub-navbar--anchor.active(href="{%url 'fund_home'%}") 基金首页
        li.sub-navbar--list-item
          a.sub-navbar--anchor(href="{% url 'fund_products' %}") 基金产品
  .panel-nopadding
    .panel-title-bar
      .panel-tabs(data-bind="foreach: tabTree")
        .panel-tab(data-bind="css:{active:$root.selectedTab() === $data}")
          a(href="#", data-bind="text:name, click:$root.selectedTab") 全部
      a.panel-title-right-item(href="{% url 'fund_products' %}") 更多
    .panel-content
      // ko template: {name: 'data-table', data: fundTable}
      // /ko

  include include/footer
  include include/templates
  script.
    require(['/static/js/fund_home.js'])
  include include/tracking