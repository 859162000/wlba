doctype html
- with '战略合作伙伴' as title
include include/head
- endwith
body
  include include/header
  include include/navigation
  .container.margin-bottom-xxl
    .siderbar-div
      - with '战略合作伙伴' as title
      include include/about_sidebar
      - endwith
    .panel-padding.span9-omega
      .well-s
        h1.about-title 战略合作伙伴
        .partner-container.partner-container-about
          ul.partner-logo.partner-logo-about
            for partner in partners
              li
                a(href="{{partner.link}}",target="_blank", title="{{partner.name}}", rel="nofollow")
                  img(src="/media/{{partner.image}}", alt="{{partner.name}}")

  include include/footer
  include include/tracking
